<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jy.myblog.user.UserMapper">
    <select id="getUser">
        SELECT iuser, uid, upw, nm, role
        FROM user
        WHERE uid = #{uid}
    </select>
</mapper>